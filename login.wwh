  procedures :
   -
     name : Connection
     procedure_id : 1287594520629088264
     type_code : 14
     code : |1-
      
      // Summary: Starts the connection procedure
      //
      Procédure connection()
      
      bOnUser is boolean = False
      
      // Search for the user
      HReadSeek(BlogUser,Login,EDT_Login)
      if HFound(BlogUser) then
      	bOnUser = True
      else
      	HReadSeek(BlogUser,Email,EDT_Login)
      	if HFound(BlogUser) then
      		bOnUser = True
      	end
      end
      
      // Check the password
      if bOnUser then
      	if BlogUser.Password = EDT_Pwd then
      		gnCurrentUserID = BlogUser.BlogUserID
      		PageDisplay(dashboard)
      	end
      end
      	
      EDT_Pwd = ""
      STC_Error = "Invalid user or password"
      STC_Error..Visible = True
     type : 458752
   -
     name : ForgottenPassword
     internal_properties : CAAAAAgAAACATKNeB/mPQ4nEFEwsAqSR0Ep0u8iX1H2eQGrR5KYDFLmvmeq0lxItr5Tq7i/9PzEyz4Tr7GEVvYgMF8ggWZRf3KpRFzFeND+HV8CD1muSUJTIC6q4nDtFEACX3S8WRrplc18UMXtHYW6oEqOlO2LM0MtE3JB0gUj+kJ6Q8P18IYlQqV4DTCn4NCd4aBVSkh+89qyVmsQT
     procedure_id : 1290238180368731856
     type_code : 14
     code : |1+
      
      // Summary: Sends the password to the user
      //// Automatic procedure:
      // The procedure is run manually, during a call in the code
      // The execution will be done at the end of the process containing the call
      // It will be run in a thread (without having to call ThreadExecute), without using HFSQL
      // 
      
      Procédure ForgottenPassword()
      
      bOnUser is boolean = False
      
      sEmail is string = [
      	Dear %1, 
      	
      	Please find below your forgotten password.
      	
      	Password: %2
      	
      	%3 team.
      ]
      
      
      MySession is an emailSMTPSession
      MyMessage is an Email
      
      EmailSetTimeOut(10)
      
      // Setting of sessionSession.ServerAddress = LoadPref(SMTP_SERVER_ADDRESS)
      MySession..Name = LoadPref(SMTP_USER)
      MySession..Password = LoadPref(SMTP_PASSWORD,"",True)
      MySession..Option = LoadPref(SMTP_OPTION,""+emailOptionDefault )
      MySession..Port = LoadPref(SMTP_PORT,"25")
      
      if EmailStartSession(MySession) then
      	// Session started
      	MyMessage..Sender = LoadPref(SMTP_SENDER)
      	Add(MyMessage..Recipient,BlogUser.Email)
      	MyMessage..Subject = "Forgotten password"
      	MyMessage..Message = StringBuild(sEmail,BlogUser.DisplayName,BlogUser.Password,LoadPref(COL_Preferences.BLOG_TITLE,"My blog"))
      	
      	if EmailSendMessage(MySession,MyMessage)
      		// Message sent
      		STC_Error..Visible  = False
      		return	
      	else
      		// Error while sending the message"
      	end
      	
      	EmailCloseSession(MySession)
      else
      	// Unable to start a session with the SMTP server
      end
      
     type : 458752
